{
  "name": "uiowa-header-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build @uiowa/uiowa-header",
    "build:gh-pages": "ng build --base-href /uiowa-header-demo/",
    "watch": "ng build --watch --configuration development"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
